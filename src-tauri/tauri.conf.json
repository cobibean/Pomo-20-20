{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "productName": "20-20-20 Focus Timer",
  "version": "0.1.0",
  "identifier": "com.twenty202020.focustimer",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "20-20-20 Focus Timer",
        "width": 500,
        "height": 700,
        "resizable": true,
        "fullscreen": false,
        "minWidth": 400,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
